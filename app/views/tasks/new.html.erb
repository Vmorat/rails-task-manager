<h1>Add a new task</h1>
<%= form_with model: @task do |f| %>
  <div class= "container my-4">
  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control' %>
  </div>
  <div class= "container">
  <%= f.label :details %>
  <%= f.text_field :details, class: 'form-control' %>
  <%= f.submit class: 'btn btn-primary my-2'%>
  </div>
<% end %>
